<h1>Employees#index</h1>
<p>Find me in app/views/employees/index.html.erb</p>

<table>
  <% @employees.each do |employee| %>
    <tr>
      <td>
        <%= link_to employee.full_name, employee %>
      </td>
      <td>
        <%= link_to "delete", employee, :method => :delete, :confirm => "Are you sure?" %>
      </td>
    </tr>
  <% end %>
</table>
