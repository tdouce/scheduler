<!DOCTYPE html>
<html>
<head>
  <title>Scheduler</title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

  <ul>
    <li> <%= link_to "Dashboard", root_url %> </li>
  </ul>

  <% flash.each do |key, value| %>
    <%= content_tag(:div, value, :class => "flash-#{key}"); %>
  <% end %>

<%= yield %>

</body>
</html>
